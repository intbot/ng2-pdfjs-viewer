{
  "name": "ng2-pdfjs-viewer",
  "version": "25.0.10",
  "description": "The most comprehensive Angular PDF viewer powered by Mozilla PDF.js. 7M+ downloads, mobile-first, production-ready with complete rewrite in v25.x",
  "author": {
    "name": "Aneesh Goapalakrishnan",
    "email": "codehippie1@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intbot/ng2-pdfjs-viewer"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/fesm2022/ng2-pdfjs-viewer.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/ng2-pdfjs-viewer.mjs"
    }
  },
  "devDependencies": {
    "ng-packagr": "^20.0.0",
    "shx": "^0.3.4"
  },
  "peerDependencies": {
    "@angular/common": ">=10.0.0",
    "@angular/core": ">=10.0.0"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "homepage": "https://angular-pdf-viewer-docs.vercel.app/",
  "bugs": {
    "url": "https://github.com/intbot/ng2-pdfjs-viewer/issues"
  },
  "keywords": [
    "angular",
    "angular-pdf-viewer",
    "pdf-viewer",
    "pdf-reader",
    "PDF",
    "PDF Viewer",
    "pdfjs",
    "viewerjs",
    "ng2",
    "angular-component",
    "angular-pdf",
    "ng2-pdf",
    "angular 2 - 20",
    "angular 20",
    "angular-library",
    "typescript",
    "javascript",
    "pdf-rendering",
    "pdf-annotations",
    "pdf-zoom",
    "pdf-printing",
    "responsive-ui",
    "custom PDF Viewer",
    "accessibility",
    "open-source"
  ],
  "license": "Apache-2.0 + Commons Clause",
  "readme": "README.md",
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build": "npm run clean && ng-packagr -p ng-package.json",
    "package": "npm run build && cd dist && npm pack",
    "bump": "mkdir .git && npm version patch",
    "version": "npm run build && git add -A .",
    "postversion": "git push && git push --tags && rmdir .git"
  }
}